{
  "name": "vscode-memfs",
  "displayName": "FileSystem Consumer Sample",
  "description": "Showcase for using the workspace.fs.-API which allows reading and writing files and folder",
  "version": "0.0.1",
  "publisher": "vscode-samples",
  "repository": {
    "url": "https://github.com/Microsoft/vscode-extension-samples.git",
    "type": "git"
  },
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "GPFS.workspaceInit",
        "title": "GPFS Setup Workspace"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "GPFS.workspaceInit"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "tslint -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^7.0.43",
    "@types/node-fetch": "^2.5.2",
    "@types/url-parse": "^1.4.3",
    "@types/vscode": "^1.38.0",
    "tslint": "^5.16.0",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "node-fetch": "^2.6.0",
    "url-parse": "^1.4.7"
  }
}
